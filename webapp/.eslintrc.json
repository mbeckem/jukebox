{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": [
        "react"
    ],
    "globals": {

    },
    "rules": {
        "eqeqeq": "error",
        "indent": ["error", 4],
        "react/jsx-uses-vars": "error",
        "semi": ["error", "always"],
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}
